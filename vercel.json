{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.py",
            "headers": {
                "Cache-Control": "public, max-age=0, must-revalidate"
            }
        }
    ]
}